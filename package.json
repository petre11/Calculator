{
  "name": "calculator",
  "version": "2.0.0",
  "scripts": {
    "prebuild": "pika-web --dest frontend/web_modules",
    "format": "prettier --print-width 120 --write *.js",
    "build": "parcel build index.html **/index.html",
    "test": "tape -r esm test/*Test.js | tap-spec",
    "start": "http-server"
  },
  "dependencies": {
    "@hyperapp/events": "0.0.3",
    "htm": "2.2.1",
    "hyperapp": "2.0.1",
    "hyperapp-fx": "2.0.0-beta.1"
  },
  "devDependencies": {
    "@pika/web": "0.5.3",
    "esm": "3.2.25",
    "parcel": "1.12.3",
    "prettier": "1.18.2",
    "tap-spec": "5.0.0",
    "tape": "4.11.0"
  }
}
